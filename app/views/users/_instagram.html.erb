<% instagram.images.each do |image|%>
<%= image_tag image.file, class: "img img-fluid" %>
<% end %>
<% instagram.videos.each do |video|%>
<div class="embed-responsive embed-responsive-1by1">
  <iframe class="embed-responsive-item" src=<%=video.file %> allowfullscreen></iframe>
</div>
<% end %>

<div class="post-details-user">
  <div class="post-actions-user">
    <i class="fa fa-heart like fa-2x"></i>
    <i class="fa fa-comment fa-2x post-comment"></i>
  </div>
  <p><strong><%= instagram.like_counter%> likes</strong></p>
  <%= simple_format instagram.content%>
  <p class="text-muted"><%= time_ago_in_words instagram.created_at%> ago</p>
</div>
